<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.miniproject.dao.IChatDAO">
  
	<insert id="sendMessage" parameterType="com.miniproject.model.ChatVO">
  		INSERT INTO chat(memId, sender, sendMsg, sndImg)
  		VALUES (#{memId},  #{sender}, #{sendMsg}, #{sndImg})
  	</insert>
	  	

  </mapper>
  
  
  